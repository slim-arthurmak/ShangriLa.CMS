<?xml version="1.0" encoding="utf-8"?>
<module>
  <assemblies>
    <!-- This adds the Alloy template assembly to the "default module" -->
    <add assembly="Shangri-La.EpiServer.SL.Web" />
  </assemblies>
  <clientResources>
    <add name="epi-cms.widgets.base" path="Styles/Styles.css" resourceType="Style" />
  </clientResources>
  <dojo>
    <!-- Add a mapping from shangrila to ~/ClientResources/Scripts to the dojo loader configuration -->
    <paths>
      <add name="shangrila" path="Scripts" />
      <add name="tedgustaf" path="" />
      <add name="menupin" path="Scripts/MenuPin" />
    </paths>
  </dojo>
  <dojoModules>
    <add name="visitorgroupusage" path="Scripts/visitorgroupusage" />
  </dojoModules>
  <clientModule initializer="menupin.MenuPinInit">
    <moduleDependencies>
      <add dependency="CMS" type="RunAfter" />
    </moduleDependencies>
  </clientModule>
</module>
